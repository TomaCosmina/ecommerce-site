<%-layout('layouts/principal')%>
    <link rel="stylesheet" href="/stylesheets/login.css">
    <div id="login">
        <div class="container justify-content-center form ">
            <h1 class="m-5 text-center ">Sign up here</h1>
            <p>Already have an account? <a href="/login">Log in</a></p>
            <form action="">
                <label class="form-label" for="fname"></label>
                <input class="form-control" type="text" name="fname" id="fname" placeholder="First name">
                <label class="form-label" for="lname"></label>
                <input class="form-control" type="text" name="lname" id="lname" placeholder="Last name">
                <label class="form-label" for="email"></label>
                <input class="form-control" type="text" name="email" id="email" placeholder="E-mail">
                <label class="form-label" for="password"></label>
                <input class="form-control" type="password" name="password" id="password" placeholder="Password">
                <button id="buttonLog" class="btn btn-outline-secondary mt-3 ">Log in</button>
            </form>
        </div>
    </div>
    <%-include('../partial/footer') %>